<?xml version="1.0"?>

<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <node name="left_rosparam_append_node" pkg="thor_mang_scripts" type="rosparam_append.sh" args="$(find thor_vt_hand_ros_control)/config/left_hand_controllers.yaml move_group/controller_list" />
  <rosparam file="$(find thor_vt_hand_ros_control)/config/left_hand_trajectory_controllers.yaml" command="load" />

  <!-- load the controllers -->
  <node name="left_hand_traj_controller_spawner"  pkg="controller_manager" type="spawner" output="screen"
    args="--stopped
          left_hand_traj_controller" />
</launch>
