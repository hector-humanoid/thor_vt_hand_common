<?xml version="1.0"?>

<launch>
  <arg name="namespace" default="/thor_mang"/>

  <node name    = "right_rosparam_append_node"
        pkg     = "thor_vt_hand_ros_control"
        type    = "rosparam_append"
        args    = "$(find thor_vt_hand_ros_control)/config/vt_hand_right_controllers.yaml $(arg namespace)/controller_list"/>

  <!-- load trajectory controllers -->
  <node name="trajectory_controller_right_hand_spawner" pkg="controller_manager" type="spawner" ns="$(arg namespace)" respawn="false" output="screen"
    args="right_hand_traj_controller"/>

</launch>

